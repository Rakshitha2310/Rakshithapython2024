#Union
set1 = {1, 2, 4, 7, 9}
set2 = {1, 2, 4, 6, 8}
union_set = set1|set2
print(union_set)
{1, 2, 4, 6, 7, 8, 9}

#intersection
set1 = {1, 2, 4, 7, 9}
set2 = {1, 2, 4, 6, 8}
intersection_set = set1 & set2
print(intersection_set)
{1, 2, 4}

set1 = {1, 2, 4, 7, 9}
set2 = {1, 2, 4, 6, 8}
difference_set = set1 - set2
print(difference_set)
{9, 7}
